{
  "editor.acceptSuggestionOnEnter": "on",
  "editor.suggestSelection": "first",
  "editor.snippetSuggestions": "bottom",
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.quickSuggestionsDelay": 10,
  "editor.fontSize": 14,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.formatOnPaste": true,
  "editor.multiCursorModifier": "alt",
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "editor.guides.indentation": false,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "none",
  "editor.formatOnSave": true,
  "editor.autoIndent": "advanced",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html"
  ],
  "eslint.options": {
    "extensions": [".js", ".jsx", ".ts", ".tsx", ".html"]
  },
  "[json]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[css]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Monokai Classic",
  "window.titleBarStyle": "custom",
  "window.menuBarVisibility": "toggle",
  "vim.foldfix": true,
  "vim.easymotion": true,
  "vim.easymotionKeys": "hklyuiopnm,qwertzxcvbasdgjf;",
  "vim.easymotionMarkerBackgroundColor": "black",
  "vim.easymotionMarkerFontWeight": "high",
  "vim.easymotionMarkerForegroundColorOneChar": "#e24141",
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-s>"
      ],
      "commands": [
        {
          "command": "workbench.action.files.save"
        },
        {
          "command": "extension.vim_escape"
        }
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [
        " "
      ],
      "after": [
        "M",
        "<leader>",
        "<leader>",
        "<leader>",
        "b",
        "d",
        "w"
      ]
    },
    {
      "before": [
        "Y"
      ],
      "commands": [
        "workbench.action.gotoSymbol"
      ]
    },
    {
      "before": [
        "<C-u>"
      ],
      "after": [
        "<C-b>"
      ]
    },
    {
      "before": [
        "Q"
      ],
      "commands": [
        "extension.ionSwitchTS"
      ]
    },
    {
      "before": [
        "q"
      ],
      "commands": [
        "extension.ionSwitchHTML"
      ]
    },
    {
      "before": [
        "z",
        "i"
      ],
      "commands": [
        "convert.json2ts"
      ]
    },
    {
      "before": [
        "z",
        "u"
      ],
      "commands": [
        "rest.json2ts"
      ]
    },
    {
      "before": [
        "\""
      ],
      "commands": [
        "rest-client.request"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "q"
      ],
      "after": [
        "g",
        "b"
      ]
    },
    {
      "before": [
        " "
      ],
      "after": [
        "<leader>",
        "<leader>",
        "<leader>",
        "b",
        "d",
        "w"
      ]
    }
  ],
  "vim.autoindent": true,
  "vim.useCtrlKeys": false,
  "vim.handleKeys": {
    "<C-d>": false
  },
  "files.autoSaveDelay": 2000,
  "workbench.editor.showTabs": "multiple",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.decorations.colors": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true
  },
  "breadcrumbs.enabled": true,
  "emmet.triggerExpansionOnTab": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "git.suggestSmartCommit": false,
  "extension.smoothscroll.usePercent": false,
  "extension.smoothscroll.totalDelay": 300,
  "terminal.integrated.defaultProfile.linux": "bash",
  "explorer.confirmDelete": false,
  "git.autofetch": true,
  "workbench.editorAssociations": {
    "*.wasm": "default",
    "*.pdf": "default"
  },
  "extensions.ignoreRecommendations": true,
  "git.confirmSync": false,
  "window.zoomLevel": 1,
  "remote.autoForwardPortsSource": "hybrid",
  "redhat.telemetry.enabled": true,
  "jest.runMode": "on-demand",
  "vsicons.dontShowNewVersionMessage": true,
  "database-client.autoSync": true,
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path-linux": "/home/sharifi/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm",
    "vscode-kubernetes.kubectl-path-linux": "/home/sharifi/.local/state/vs-kubernetes/tools/kubectl/kubectl"
  },
  "files.autoSave": "afterDelay",
  "github.copilot.advanced": {
    "authProvider": "github"
  },
}
